<div>
  <h3>Surveys:</h3>
  <ul>
  <% @surveys.each do |survey| %>
    <li><a href="/surveys/<%= survey.id %>"><%= survey.title %></a></li>
  <% end %>

  <% if logged_in? %>
    <li><a href='/surveys/new'>Create survey</a><li>
  <% else %>
    <li><a href='/register'>Create survey</a><li>
  <% end %>
</ul>
</div>

